-- 성적 처리
-- [ 학생 성적 처리 예제 ]

-- SpringBoot 과목평가 (기초)
-- 학생 성적 입력, 조회, 수정, 삭제
USE dima;

-- 테이블 삭제
DROP TABLE dima.student;

-- 테이블 생성
CREATE TABLE dima.student
(
	stid varchar(10) PRIMARY KEY
	, stname varchar(30) NOT NULL
	, korean int default 0
	, english int default 0
	, math int default 0
);

COMMIT;
SELECT * FROM dima.student;
 